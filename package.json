{
  "name": "atlas-pay-sdk",
  "version": "2.0.6",
  "description": "Raven Atlas NodeJS Payment SDK",
  "keywords": [
    "raven",
    "checkout",
    "card",
    "ussd",
    "bank",
    "shopping",
    "payment",
    "checkout",
    "gateway",
    "ecommerce",
    "online",
    "credit",
    "debit",
    "card",
    "processing",
    "secure",
    "transaction",
    "fraud",
    "protection",
    "API",
    "integration",
    "web",
    "mobile",
    "JavaScript",
    "npm",
    "module",
    "library",
    "atlas",
    "webpay"
  ],
  "main": "./bundles/index.cjs.js",
  "module": "./bundles/index.esm.js",
  "browser": "./index.min.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/RavenPayAfrica/atlas-webpay-node-sdk.git"
  },
  "homepage": "https://ravenpayafrica.github.io/atlas-webpay-node-sdk/",
  "author": "Ezeani Emmanuel <emmanuel.ezeani@getravenbank.com>",
  "license": "MIT",
  "engines": {
    "node": ">=8.5",
    "npm": ">=6.0",
    "yarn": "^1.0"
  },
  "scripts": {
    "cleanup": "shx rm -rf dist",
    "prebuild": "yarn cleanup && yarn verify",
    "build": "yarn build:tsc && yarn build:rollup",
    "build:tsc": "tsc && tsc --target es2018 --outDir dist/esm2018",
    "build:rollup": "rollup -c config/rollup.config.mjs --bundleConfigAsCjs && rollup -c config/rollup.config.mjs --bundleConfigAsCjs --environment NODE_ENV:production",
    "postbuild": "node scripts/copy && yarn size",
    "docs": "typedoc --entryPoints src/index.ts --exclude '**/__tests__/**/*.*' --excludeExternals --excludePrivate --excludeNotDocumented --name 'Atlas Pay SDK' --out docs",
    "test": "jest -c ./config/jest.config.js",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "test:ci": "npm t -- --ci",
    "validate-js": "tsc -p ./config  && tsc -p ./scripts",
    "verify": "yarn style ",
    "commit": "git-cz",
    "style": "yarn format && yarn lint",
    "style:fix": "yarn format:fix && yarn lint:fix",
    "format": "prettier \"**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}\"",
    "format:fix": "yarn format --write",
    "lint": "eslint",
    "lint:fix": "yarn lint --fix",
    "prerelease": "yarn build",
    "release": "standard-version",
    "postrelease": "node scripts/copy && node scripts/build && yarn release:github && yarn release:npm",
    "release:github": "git push --no-verify --follow-tags origin master",
    "release:npm": "npm publish ./dist",
    "release:preflight": "npm pack ./dist --dry-run",
    "size": "node --experimental-modules scripts/file-size ./dist/index.min.js ./dist/bundles/index.esm.js"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --config config/commitlint.config.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn style && npm test -- --bail --onlyChanged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,scss,sass,less,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "yarn lint:fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "tslib": ">=1.9.0"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "15.0.2",
    "@types/express-serve-static-core": "4.17.34",
    "@types/mime": "3.0.1",
    "atlas-pay-sdk": "2.0.2",
    "axios": "1.3.6",
    "md5": "2.3.0",
    "node-forge": "1.3.1",
    "ramda": "0.29.0",
    "text-encoding": "0.7.0"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@commitlint/cli": "17.6.1",
    "@commitlint/config-conventional": "17.6.1",
    "@rollup/plugin-babel": "6.0.3",
    "@rollup/plugin-commonjs": "24.1.0",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-replace": "5.0.2",
    "@rollup/plugin-terser": "0.4.1",
    "@rollup/plugin-typescript": "11.1.0",
    "@types/jest": "29.5.1",
    "@types/json5": "0.0.30",
    "@types/md5": "2.3.2",
    "@types/node": "18.16.1",
    "@types/node-forge": "1.3.2",
    "@types/prettier": "2.7.2",
    "@types/ramda": "0.29.0",
    "@types/webpack-config-utils": "2.3.1",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "brotli-size": "4.0.0",
    "commitizen": "4.3.0",
    "cross-var": "1.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.39.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.7.0",
    "eslint-plugin-promise": "6.1.1",
    "gzip-size": "7.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-watch-typeahead": "2.2.2",
    "kleur": "4.1.5",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "pretty-bytes": "6.1.0",
    "rollup": "3.21.0",
    "rollup-plugin-node-polyfills": "0.2.1",
    "rollup-plugin-sourcemaps": "0.6.3",
    "shx": "0.3.4",
    "standard-version": "9.5.0",
    "ts-jest": "29.1.0",
    "tslib": "2.5.0",
    "typedoc": "0.24.6",
    "typescript": "5.0.4",
    "webpack-config-utils": "2.3.1"
  }
}
